{
  "name": "simple-react-full-stack",
  "version": "1.0.0",
  "description": "Boilerplate to build a full stack web application using React, Node.js, Express and Webpack.",
  "main": "src/server/server.js",
  "scripts": {
    "build": "webpack --mode production",
    "start": "npm run build && node src/server/server.js",
    "client": "webpack-dev-server --mode development --https --devtool inline-source-map --hot",
    "server": "nodemon src/server/server.js",
    "dev": "set HTTPS=true&&concurrently \"npm run server\" \"npm run client\"",
    "seed": "node src/server/models/seed.js"
  },
  "author": "Renzo Cotti",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.4",
    "body-parser": "^1.18.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "express": "^4.16.4",
    "express-session": "^1.15.6",
    "file-loader": "^3.0.1",
    "materialize-css": "^1.0.0",
    "mongoose": "^5.4.6",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-materialize": "^3.0.3",
    "react-redux": "^6.0.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "stopword": "^0.1.13",
    "webpack-cli": "^3.2.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "nodemon": "^1.17.3",
    "style-loader": "^0.23.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-dev-server": "^3.1.3"
  }
}
